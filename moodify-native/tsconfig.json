{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "allowJs": true,
    "jsx": "react-native"
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/+not-found.jsx",
    "app/(tabs)/index.jsx",
    "app/(tabs)/explore.jsx",
    "app/(tabs)/_layout.jsx",
    "app/_layout.js",
    "nativewind-env.d.ts"
  ]
}